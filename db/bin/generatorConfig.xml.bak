<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="..\..\WebContent\WEB-INF\lib\mysql-connector-java-5.1.30.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/testplatform?useUnicode=true&amp;characterEncoding=utf-8"
			userId="root" password="root" />
		<javaModelGenerator targetPackage="com.cpit.testplatform.modules.backup.entity"
			targetProject="..\..\src" />
		<sqlMapGenerator targetPackage="backup"
			targetProject="..\..\resources\mappings\modules" />
		<javaClientGenerator targetPackage="com.cpit.testplatform.modules.backup.dao"
			targetProject="..\..\src" type="XMLMAPPER" />
 
		<table schema="" tableName="backup_plan" domainObjectName="BackupPlan"/>
		<table schema="" tableName="backup_history" domainObjectName="BackupHistory"/>
<!--		<table schema="" tableName="term_archive" domainObjectName="TerminalArchive"/>-->
	</context>
</generatorConfiguration>